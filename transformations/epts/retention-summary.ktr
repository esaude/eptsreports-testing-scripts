    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.util.*;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  Object[] r = getRow();

  if (r != null) {
    r = createOutputRow(r, data.outputRowMeta.size());
    List artStartDates = new ArrayList();
	Date startDate = get(Fields.In, "start_date").getDate(r);
	Date endDate = get(Fields.In, "end_date").getDate(r);
	addIfNotNull(get(Fields.In, "art_start_drugs_date").getDate(r), artStartDates, startDate, endDate);
	addIfNotNull(get(Fields.In, "art_historic_start_date").getDate(r), artStartDates, startDate, endDate);
	addIfNotNull(get(Fields.In, "art_program_start_date").getDate(r), artStartDates, startDate, endDate);
	addIfNotNull(get(Fields.In, "art_drug_pickup_date").getDate(r), artStartDates, startDate, endDate);
    Collections.sort(artStartDates);
    if (!artStartDates.isEmpty()) {
      get(Fields.Out, "art_effective_start_date").setValue(r, artStartDates.get(0));
	}
    putRow(data.outputRowMeta, r);
    return true;
  } else {
    setOutputDone();
    return false;
  }

}

private void addIfNotNull(Date value, Collection collection, Date startDate, Date endDate) {
	if (value != null &amp;&amp; startDate.compareTo(value) &lt;= 0 &amp;&amp; endDate.compareTo(value) >= 0) {
		collection.add(value);
	}
}
