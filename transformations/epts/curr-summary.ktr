    <connection>Openmrs</connection>
    <rowlimit>1</rowlimit>
    <sql>select patient_state.state abandonment_code, date(patient_state.start_date) abandonment_date from patient_program
join patient_state on patient_state.patient_program_id = patient_program.patient_program_id
where patient_program.program_id = 2
  and patient_program.patient_id = ?
  and patient_state.state in (7, 8, 9, 10)
  and patient_state.end_date is null
  and patient_state.start_date &lt;= ?
  and patient_program.location_id = ?
  and patient_program.voided = false
  and patient_state.voided = false
</sql>
